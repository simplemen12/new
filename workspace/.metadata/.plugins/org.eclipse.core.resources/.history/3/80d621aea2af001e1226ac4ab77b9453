package day06;

public class ArrayEx02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 1~50까지의 랜덤수를 10개 생성하여 배열에 저장 출력 
		 * 오름차순 정렬 후 출력
		 */
		int arr[] = new int[10];
		
		 for(int i=0; i<arr.length; i++) {
			arr[i] = (int) (Math.random() * 50) + 1;
			System.out.print(arr[i]);
		 }
		

	}

}
